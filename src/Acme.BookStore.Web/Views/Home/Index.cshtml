@model List<Acme.BookStore.Authors.AuthorDto>
@{
    Layout = null;
}

    @*<abp-script src="/Pages/Authors/Index.js"/>*@
   




<from>


    <table class="table  table-active table-success">
        <tr>
            <th>姓名</th>
            <th>出生日期</th>
        </tr>

        @foreach (var author in Model)
        {
            <tr>
                <td>@author.Name</td>
                <td>@author.BirthDate</td>
            </tr>
        }


    </table>
</from>
<input type="text" name="WorkPlanTransfer" class="WorkPlanTransfer" value="" />
